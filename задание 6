class Employee():
    def __init__(self, name, age, salary):
      employee1 = Employee()
      employee2 = Employee()
      employee1.name = 'john'
      employee2.name = 'eric'
      employee1.salary = '67000'
      employee2.salary = '69000'
    def show(self, name, surname):
      return name + ' ' + surname
      employee1.surname = 'smit'
      employee2.surname = 'shishebarov'
print(employee1.show('john', 'smit'))
print(employee1.salary)

